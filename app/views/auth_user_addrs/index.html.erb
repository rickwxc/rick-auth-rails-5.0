<p id="notice"><%= notice %></p>

<h1>Auth User Addrs</h1>

<table class='table'>
  <thead>
    <tr>
      <th>Auth user</th>
      <th>Auth user addr type</th>
      <th>Auth addr</th>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Mobile</th>
      <th>Company</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auth_user_addrs.each do |auth_user_addr| %>
      <tr>
        <td><%= auth_user_addr.auth_user_id %><br />
        <%= auth_user_addr.auth_visitor_uuid %></td>
        <td><%= auth_user_addr.auth_user_addr_type %></td>
        <td><%= auth_user_addr.auth_addr_id %></td>
        <td><%= auth_user_addr.firstname %></td>
        <td><%= auth_user_addr.lastname %></td>
        <td><%= auth_user_addr.mobile %></td>
        <td><%= auth_user_addr.company %></td>
        <td><%= link_to 'Show', auth_user_addr %></td>
        <td><%= link_to 'Edit', edit_auth_user_addr_path(auth_user_addr) %></td>
        <td><%= link_to 'Destroy', auth_user_addr, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auth User Addr', new_auth_user_addr_path %>
