<p id="notice"><%= notice %></p>

<h1>Auth Bundle Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auth_bundle_types.each do |auth_bundle_type| %>
      <tr>
        <td><%= auth_bundle_type.name %></td>
        <td><%= link_to 'Show', auth_bundle_type %></td>
        <td><%= link_to 'Edit', edit_auth_bundle_type_path(auth_bundle_type) %></td>
        <td><%= link_to 'Destroy', auth_bundle_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auth Bundle Type', new_auth_bundle_type_path %>
