<p id="notice"><%= notice %></p>

<h1>Auth Obj Attr Groups</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auth_obj_attr_groups.each do |auth_obj_attr_group| %>
      <tr>
        <td><%= auth_obj_attr_group.name %></td>
        <td><%= link_to 'Show', auth_obj_attr_group %></td>
        <td><%= link_to 'Edit', edit_auth_obj_attr_group_path(auth_obj_attr_group) %></td>
        <td><%= link_to 'Destroy', auth_obj_attr_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

