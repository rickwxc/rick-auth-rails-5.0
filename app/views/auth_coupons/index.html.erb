<p id="notice"><%= notice %></p>

<h1>Auth Coupons</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Val</th>
      <th>Expire at</th>
      <th>Is enabled</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auth_coupons.each do |auth_coupon| %>
      <tr>
        <td><%= auth_coupon.code %></td>
        <td><%= auth_coupon.name %></td>
        <td><%= auth_coupon.val %></td>
        <td><%= auth_coupon.expire_at %></td>
        <td><%= auth_coupon.is_enabled %></td>
        <td><%= link_to 'Show', auth_coupon %></td>
        <td><%= link_to 'Edit', edit_auth_coupon_path(auth_coupon) %></td>
        <td><%= link_to 'Destroy', auth_coupon, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auth Coupon', new_auth_coupon_path %>
