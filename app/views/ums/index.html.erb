<p id="notice"><%= notice %></p>

<h1>Ums</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Content</th>
      <th>Provider</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ums.each do |um| %>
      <tr>
        <td><%= um.user %></td>
        <td><%= um.content %></td>
        <td><%= um.provider %></td>
        <td><%= link_to 'Show', um %></td>
        <td><%= link_to 'Edit', edit_um_path(um) %></td>
        <td><%= link_to 'Destroy', um, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Um', new_um_path %>
