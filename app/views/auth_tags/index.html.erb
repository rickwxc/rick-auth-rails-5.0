<p id="notice"><%= notice %></p>

<h1>Auth Tags</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Auth tagtype</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auth_tags.each do |auth_tag| %>
      <tr>
        <td><%= auth_tag.name %></td>
        <td><%= auth_tag.auth_tagtype_id %></td>
        <td><%= link_to 'Show', auth_tag %></td>
        <td><%= link_to 'Edit', edit_auth_tag_path(auth_tag) %></td>
        <td><%= link_to 'Destroy', auth_tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auth Tag', new_auth_tag_path %>
