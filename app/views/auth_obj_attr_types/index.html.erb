<p id="notice"><%= notice %></p>

<h1>Auth Obj Attr Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Auth obj attr group</th>
      <th>Idx</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auth_obj_attr_types.each do |auth_obj_attr_type| %>
      <tr>
        <td><%= auth_obj_attr_type.name %></td>
        <td><%= auth_obj_attr_type.show_name %></td>
        <td><%= auth_obj_attr_type.is_show %></td>
        <td><%= auth_obj_attr_type.auth_obj_attr_group_id %></td>
        <td><%= auth_obj_attr_type.idx %></td>
        <td><%= link_to 'Show', auth_obj_attr_type %></td>
        <td><%= link_to 'Edit', edit_auth_obj_attr_type_path(auth_obj_attr_type) %></td>
        <td><%= link_to 'Destroy', auth_obj_attr_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auth Obj Attr Type', new_auth_obj_attr_type_path %>
