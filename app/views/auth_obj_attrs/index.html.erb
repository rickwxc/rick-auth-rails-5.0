<p id="notice"><%= notice %></p>

<h1>Auth Obj Attrs</h1>

<table>
  <thead>
    <tr>
      <th>Model</th>
      <th>Obj</th>
      <th>Auth type</th>
      <th>Value</th>
      <th>Idx</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auth_obj_attrs.each do |auth_obj_attr| %>
      <tr>
        <td><%= auth_obj_attr.model %></td>
        <td><%= auth_obj_attr.obj_id %></td>
        <td><%= auth_obj_attr.auth_attr_type_id %></td>
        <td><%= auth_obj_attr.value %></td>
        <td><%= auth_obj_attr.idx %></td>
        <td><%= link_to 'Show', auth_obj_attr %></td>
        <td><%= link_to 'Edit', edit_auth_obj_attr_path(auth_obj_attr) %></td>
        <td><%= link_to 'Destroy', auth_obj_attr, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auth Obj Attr', new_auth_obj_attr_path %>
