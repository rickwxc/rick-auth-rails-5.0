<p id="notice"><%= notice %></p>

<h1>Saleable Items</h1>

<table class='table'>
  <thead>
    <tr>
      <th> name</th>
      <th> uuid</th>
      <th> stock</th>
      <th> in stock date</th>
      <th> unit price</th>
      <th> is disabled</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auth_objs.each do |auth_obj| %>
      <tr>
		  <td> <%= auth_obj.auth_name %> </td>
        <td><%= auth_obj.auth_uuid %></td>
        <td><%= auth_obj.stock_qty %></td>
		<td>
			<%= auth_obj.in_stock_date %>
			<br />
			<%if auth_obj.auth_is_presale %>
				Pre-sale
			<%end %>
	
	</td>
        <td><%= auth_obj.auth_unitprice %></td>
        <td><%= (auth_obj.auth_is_disabled  == true ? 'Disabled':'') %></td>
		<td>
			<a target='_blank'
				href="/auth_bundle2objs/new?auth_obj_model_name=<%=auth_obj.class.name%>&auth_obj_id=<%=auth_obj.id%>">
				Add to bundle
			</a>
			|
<%= link_to 'Add Img', new_img_path(:uuid => auth_obj.id, :modelname => auth_obj.class.name), target: "_blank"  %>
		</td>

        <td><%= link_to 'Show', auth_obj %></td>
        <td><%= link_to 'Edit', edit_auth_obj_path(auth_obj) %></td>
        <td><%= link_to 'Destroy', auth_obj, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @auth_objs %>

<br>

<%= link_to 'New Auth Obj', new_auth_obj_path %>
